{
	"manifest_version": 1,
	"id": "module_manager",
	"name": "Module manager",
	"author": "Zabbix",
	"description": "Manager for modules",
	"version": "1.0",
	"namespace": "Example",
	"url": "https://zabbix.com",
	"actions": {
		"module.list": {
			"class": "ModuleListAction",
			"view": "views/administration.module.list"
		},
		"module.edit": {
			"class": "ModuleFormAction",
			"view": "administration.module.edit"
		},
		"module.update": {
			"class": "ModuleUpdateAction"
		},
		"module.enable": {
			"class": "ModuleUpdateAction"
		},
		"module.disable": {
			"class": "ModuleUpdateAction"
		},
		"module.scan": {
			"class": "ModuleScanAction"
		}
	}
}
